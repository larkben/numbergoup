{
  "name": "numbergoup",
  "version": "0.1.0",
  "license": "GPL",
  "scripts": {
    "dev": "next dev",
    "start": "next start",
    "export": "next export",
    "build": "npm run mainnet:build",
    "devnet:build": "NEXT_PUBLIC_NETWORK=devnet next build",
    "testnet:build": "NEXT_PUBLIC_NETWORK=testnet next build",
    "mainnet:build": "NEXT_PUBLIC_NETWORK=mainnet next build",
    "compile": "npx --yes @alephium/cli@latest compile",
    "deploy": "npm run devnet:deploy",
    "devnet:deploy": "npx --yes @alephium/cli@latest deploy -n devnet",
    "testnet:deploy": "npx --yes @alephium/cli@latest deploy -n testnet",
    "mainnet:deploy": "npx --yes @alephium/cli@latest deploy -n mainnet",
    "lint": "next lint",
    "lint:fix": "next lint --fix",
    "test": "jest -i --config ./jest-config.json",
    "prettier": "prettier --write \"**/*.{js,jsx,ts,tsx,json,md}\""
  },
  "prettier": {
    "printWidth": 120,
    "tabWidth": 2,
    "useTabs": false,
    "semi": false,
    "singleQuote": true,
    "bracketSameLine": false,
    "trailingComma": "none"
  },
  "dependencies": {
    "@alephium/cli": "^1.0.2",
    "@alephium/web3": "^1.0.2",
    "@alephium/web3-test": "^1.0.2",
    "@alephium/web3-wallet": "^1.0.2",
    "@alephium/web3-react": "^1.0.2",
    "@alephium/token-list": "0.0.11",
    "@next/font": "13.1.4",
    "@planetscale/database": "^1.10.0",
    "detect-browser": "^5.3.0",
    "dotenv": "^16.3.1",
    "eslint-config-next": "13.1.4",
    "framer-motion": "^6.3.11",
    "i": "^0.3.7",
    "mysql2": "^3.6.3",
    "next": "^13.5.6",
    "phaser": "^3.70.0",
    "react": "18.2.0",
    "react-dom": "18.2.0",
    "react-transition-state": "^1.1.4",
    "react-type-animation": "^3.2.0",
    "react-use-measure": "^2.1.1",
    "resize-observer-polyfill": "^1.5.1",
    "styled-components": "^5.3.5",
    "three": "^0.159.0",
    "typewriter-effect": "^2.21.0"
  },
  "devDependencies": {
    "@types/jest": "^27.5.1",
    "@types/node": "18.11.18",
    "@types/react": "18.0.27",
    "@types/react-dom": "18.0.10",
    "@types/styled-components": "^5.1.25",
    "@types/three": "^0.159.0",
    "@typescript-eslint/eslint-plugin": "^5.51.0",
    "@typescript-eslint/parser": "^5.51.0",
    "eslint": "8.34.0",
    "eslint-config-prettier": "^8.6.0",
    "eslint-plugin-prettier": "^4.2.1",
    "jest": "^29.4.2",
    "ts-jest": "^29.0.5",
    "ts-node": "^10.9.1",
    "typescript": "^4.4.2"
  },
  "engines": {
    "node": "20.x",
    "npm": ">=7.0.0"
  }
}
